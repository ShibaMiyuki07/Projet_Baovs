<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertionEtudiant</servlet-name>
        <servlet-class>Page.InsertionEtudiant</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertionFiliere</servlet-name>
        <servlet-class>Page.InsertionFiliere</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListeCandidatInfo</servlet-name>
        <servlet-class>Page.ListeCandidatInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListeCandidatDesign</servlet-name>
        <servlet-class>Page.ListeCandidatDesign</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertionEtudiant</servlet-name>
        <url-pattern>/InsertionEtudiant</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ListeInfo</servlet-name>
        <servlet-class>Page.ListeInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListeInfo</servlet-name>
        <url-pattern>/ListeInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertionFiliere</servlet-name>
        <url-pattern>/InsertionFiliere</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeCandidatInfo</servlet-name>
        <url-pattern>/ListeCandidatInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListeCandidatDesign</servlet-name>
        <url-pattern>/ListeCandidatDesign</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
